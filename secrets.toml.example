# Configurações do Streamlit Secrets
# Copie este arquivo para .streamlit/secrets.toml e preencha com suas credenciais
# Este arquivo tem prioridade sobre variáveis de ambiente

[db]
host = "localhost"
port = 5432
dbname = "plane"
user = "seu_usuario"
password = "sua_senha_segura"

# Configurações adicionais (opcional)
[server]
enableCORS = false
enableXsrfProtection = false

# Exemplo de outras configurações que você pode adicionar:
# [api]
# key = "sua_chave_api"
# 
# [auth]
# username = "admin"
# password = "senha_admin"